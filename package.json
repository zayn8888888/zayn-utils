{
  "name": "zayn-utils",
  "version": "1.0.9",
  "description": "A utility library",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "src/plugins"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc",
    "build:all": "npm run build:types && npm run build",
    "prepublishOnly": "npm run build:all",
    "test": "npm run build:all && npm pack",
    "dev": "npm run build:types -- --watch",
    "obfuscator": "javascript-obfuscator index.js --output main.js --compact true --control-flow-flattening true --string-array true --string-array-encoding base64 --self-defending true"
  },
  "keywords": [
    "utilities",
    "tools"
  ],
  "author": "Zayn",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "chalk": "^5.4.1",
    "https-proxy-agent": "^7.0.6",
    "marked": "^15.0.12",
    "node-fetch": "^3.3.2",
    "readline": "^1.3.0",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/node": "^24.0.1",
    "rollup": "^4.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "matrix-js-sdk": "37.5.0",
    "puppeteer": "^24.10.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "windows-shortcuts": "^0.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
