{
   "action": {
      "default_popup": "index.html#/popup"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "css": [ "content-scripts/scholar.css" ],
      "js": [ "content-scripts/scholar.js" ],
